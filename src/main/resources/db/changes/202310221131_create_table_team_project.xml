<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <changeSet id="CREATE TABLE TEAM_PROJECT" author="witczakm">
        <createTable tableName="TEAM_PROJECT">
            <column name="project_id" type="bigint">
                <constraints references="PROJECT(id)"
                             foreignKeyName="FK_TEAM_PROJECT_PROJECT"
                             primaryKey="true" primaryKeyName="PK_PERSON_TEAM"/>
            </column>
            <column name="team_id" type="bigint">
                <constraints references="TEAM(id)"
                             foreignKeyName="FK_TEAM_PROJECT_TEAM"
                             primaryKey="true" primaryKeyName="PK_PERSON_TEAM"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>