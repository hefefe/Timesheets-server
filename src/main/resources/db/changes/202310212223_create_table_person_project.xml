<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <changeSet id="CREATE TABLE PERSON_PROJECT" author="witczakm">
        <createTable tableName="PERSON_PROJECT">
            <column name="person_id" type="bigint">
                <constraints references="PERSON(id)"
                             foreignKeyName="FK_PERSON_PROJECT_PERSON"
                             primaryKey="true" primaryKeyName="PK_PERSON_PROJECT"/>
            </column>
            <column name="project_id" type="bigint">
                <constraints references="PROJECT(id)"
                             foreignKeyName="FK_PERSON_PROJECT_PROJECT"
                             primaryKey="true" primaryKeyName="PK_PERSON_PROJECT"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>