<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <changeSet id="CREATE TABLE PERSON_TEAM" author="witczakm">
        <createTable tableName="PERSON_TEAM">
            <column name="person_id" type="bigint">
                <constraints references="PERSON(id)"
                             foreignKeyName="FK_PERSON_TEAM_PERSON"
                             primaryKey="true" primaryKeyName="PK_PERSON_TEAM"/>
            </column>
            <column name="team_id" type="bigint">
                <constraints references="TEAM(id)"
                             foreignKeyName="FK_PERSON_TEAM_TEAM"
                             primaryKey="true" primaryKeyName="PK_PERSON_TEAM"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>